{
  "name": "UnifiedExtensionForFemiwiki",
  "version": "1.0.0",
  "author": "[https://femiwiki.com/ Femiwiki Team]",
  "url": "https://github.com/femiwiki/unifiedExtensionForFemiwiki",
  "descriptionmsg": "unifiedextensionforfemiwiki-desc",
  "license-name": "AGPL-3.0-or-later",
  "type": "specialpage",
  "requires": {
    "MediaWiki": ">= 1.35.0",
    "extensions": {
      "CategoryTree": "*"
    }
  },
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\": "includes/"
  },
  "AuthManagerAutoConfig": {
    "preauth": {
      "FemiwikiPreAuthenticationProvider": {
        "class": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\FemiwikiPreAuthenticationProvider"
      }
    }
  },
  "Hooks": {
    "BeforePageDisplay": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Hooks::onBeforePageDisplay",
    "HtmlPageLinkRendererBegin": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Hooks::onHtmlPageLinkRendererBegin",
    "LinkerMakeExternalLink": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Hooks::onLinkerMakeExternalLink",
    "SidebarBeforeOutput": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Hooks::onSidebarBeforeOutput",
    "SkinAddFooterLinks": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Hooks::onSkinAddFooterLinks"
  },
  "SpecialPages": {
    "OrderedWhatlinkshere": {
      "class": "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Specials\\SpecialOrderedWhatLinksHere",
      "services": [
        "DBLoadBalancer",
        "LinkBatchFactory",
        "ContentHandlerFactory",
        "SearchEngineFactory",
        "NamespaceInfo"
      ]
    }
  },
  "MessagesDirs": {
    "UnifiedExtensionForFemiwiki": ["i18n"]
  },
  "config": {
    "GoogleAnalyticsTrackingID": {
      "value": ""
    }
  },
  "TestAutoloadNamespaces": {
    "MediaWiki\\Extension\\UnifiedExtensionForFemiwiki\\Tests\\Unit\\": "tests/phpunit/unit/"
  },
  "manifest_version": 2
}
